{
	"locale": "de",
	"format": "%organization\n%recipient\n%addressLine1\n%addressLine2\n%postalCode %locality",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea",
		"postalCode"
	],
	"uppercase_fields": [
        	"administrativeArea"
	],
    	"administrative_area_type": "state",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "\\d{4}",
	"postal_code_prefix": "CH-"
}
